<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="spot-web"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
      <excludes>
        <exclude>net/savantly/spot/web/modules/SpotController.java</exclude>
        <exclude>net/savantly/spot/web/modules/SpotFixture.java</exclude>
        <exclude>net/savantly/spot/web/modules/SpotModule.java</exclude>
        <exclude>net/savantly/spot/web/SpotWebApplication.java</exclude>
      </excludes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>net/savantly/spot/web/modules/SpotController.java</include>
        <include>net/savantly/spot/web/modules/SpotFixture.java</include>
        <include>net/savantly/spot/web/modules/SpotModule.java</include>
        <include>net/savantly/spot/web/SpotWebApplication.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/resources</directory>
      <includes>
        <include>**/*.html</include>
        <include>**/*.properties</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/test/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>.gitignore</include>
        <include>LICENSE</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
